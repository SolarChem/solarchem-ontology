#################################################################
#    Classes
#################################################################

###  https://w3id.org/solar/o/core#Condition
solar:Condition rdf:type owl:Class ;
                rdfs:label "Condition" .


###  https://w3id.org/solar/o/core#ElectrochemicalConfiguration
solar:ElectrochemicalConfiguration rdf:type owl:Class ;
                                   rdfs:subClassOf solar:Condition ;
                                   rdfs:label "Electrochemical Configuration" .


###  https://w3id.org/solar/o/core#ExperimentExecution
solar:ExperimentExecution rdf:type owl:Class ;
                          rdfs:label "Experiment Execution" .


###  https://w3id.org/solar/o/core#Role
solar:Role rdf:type owl:Class ;
           rdfs:label "Role" .


###  https://w3id.org/solar/o/pec#ABPE
solarpec:ABPE rdf:type owl:Class ;
              rdfs:subClassOf solarpec:QuantumEfficiencyMetric ;
              rdfs:label "Applied Bias Photon-to-Current Efficiency (ABPE)" .


###  https://w3id.org/solar/o/pec#APCE
solarpec:APCE rdf:type owl:Class ;
              rdfs:subClassOf solarpec:QuantumEfficiencyMetric ;
              rdfs:comment "A measure of how absorbed photons transform to current" ;
              rdfs:label "Absorbed Photon-to-Current Efficiency (APCE)" .


###  https://w3id.org/solar/o/pec#ElectrochemicalMetric
solarpec:ElectrochemicalMetric rdf:type owl:Class ;
                               rdfs:label "Electrochemical Metric" .


###  https://w3id.org/solar/o/pec#FieldFactor
solarpec:FieldFactor rdf:type owl:Class ;
                     rdfs:subClassOf solarpec:ElectrochemicalMetric ;
                     rdfs:label "Field Factor" .


###  https://w3id.org/solar/o/pec#IPCE
solarpec:IPCE rdf:type owl:Class ;
              rdfs:subClassOf solarpec:QuantumEfficiencyMetric ;
              rdfs:comment "A measure of the ratio of the photocurrent (converted to an electron transfer rate) versus the rate of incident photons (converted from the calibrated power of a light source) as a function of wavelength" ;
              rdfs:label "Incident Photon-to-Current Efficiency (IPCE)" .


###  https://w3id.org/solar/o/pec#Onset
solarpec:Onset rdf:type owl:Class ;
               rdfs:subClassOf solarpec:ElectrochemicalMetric ;
               rdfs:label "Onset" .


###  https://w3id.org/solar/o/pec#PhotocurrentDensity
solarpec:PhotocurrentDensity rdf:type owl:Class ;
                             rdfs:subClassOf solarpec:ElectrochemicalMetric ;
                             rdfs:comment "The ratio of the photocurrent produced by the photoelectrode to the area irradiated by a sunlight" ;
                             rdfs:label "Photocurrent Density" .


###  https://w3id.org/solar/o/pec#PhotoelectrocatalysisExperiment
solarpec:PhotoelectrocatalysisExperiment rdf:type owl:Class ;
                                         rdfs:subClassOf solar:ExperimentExecution ;
                                         rdfs:comment "Experiment of a reaction based on the use of a semiconductor irradiated by light energy equal or greater than its band gap energy simultaneously biased by a gradient potential" ;
                                         rdfs:label "Photoelectrocatalysis Experiment" .


###  https://w3id.org/solar/o/pec#QuantumEfficiencyMetric
solarpec:QuantumEfficiencyMetric rdf:type owl:Class ;
                                 rdfs:comment "The ratio of the number of carriers collected by the solar cell to the number of photons of a given energy incident on the solar cell" ;
                                 rdfs:label "Quantum Efficiency Metric" .


