    dcterms:creator <http://data.example.com/author/C-Y.%20Hu-2390>,
        <http://data.example.com/author/C-Y.%20Sun-2392>,
        <http://data.example.com/author/J.%20Zhou-2391>,
        <http://data.example.com/author/M-M.%20Chen-2393>,
