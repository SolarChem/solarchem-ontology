<https://doi.org/10.1007%2Fs12274-014-0564-2> a ns1:Article ;
    ns2:hasMaterialTransformationProcess <http://data.example.com/materialTransformastionProcess/2301> ;
    dcterms:creator <http://data.example.com/author/D.%20Jiang-1731>,
