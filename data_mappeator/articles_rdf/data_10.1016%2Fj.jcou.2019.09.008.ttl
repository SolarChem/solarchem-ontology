    dcterms:creator <http://data.example.com/author/André%20E.%20Nogueira-3647>,
        <http://data.example.com/author/Caue%20Ribeiro-3652>,
        <http://data.example.com/author/Gelson%20T.S.T.%20da%20Silva-3648>,
        <http://data.example.com/author/Juliana%20A.%20Torres-3646>,
