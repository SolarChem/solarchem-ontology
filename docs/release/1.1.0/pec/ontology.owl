        <skos:narrower rdf:resource="https://w3id.org/nfdi4cat/voc4cat_0000045"/>
        <skos:narrower rdf:resource="https://w3id.org/nfdi4cat/voc4cat_0000066"/>
        <skos:prefLabel xml:lang="en">photocatalyst</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/nfdi4cat/voc4cat_0007254 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/nfdi4cat/voc4cat_0007254">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
